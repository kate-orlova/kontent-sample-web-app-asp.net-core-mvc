{
  "AppConfiguration": {
    "KenticoKontentUrl": "https://app.kontent.ai/",
    "DefaultDeliveryOptions": {
      "ProjectId": "YOUR_PROJECT_ID"
    }
  },
  "DeliveryOptions": {
    "ProductionEndpoint": "https://deliver.kontent.ai/",
    "PreviewEndpoint": "https://preview-deliver.kontent.ai/",
    "ProjectId": "YOUR_PROJECT_ID",
    "PreviewApiKey": null,
    "UsePreviewApi": false,
    "WaitForLoadingNewContent": false,
    "UseSecureAccess": false,
    "SecureAccessApiKey": null,
    "EnableRetryPolicy": true,
    "DefaultRetryPolicyOptions": {
      "DeltaBackoff": "00:00:01",
      "MaxCumulativeWaitTime": "00:00:30"
    },
    "IncludeTotalCount": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
